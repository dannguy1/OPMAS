# API Configuration
# The base URL for the backend API
VITE_API_URL=http://192.168.10.8:8000

# Authentication Configuration
# JWT token expiration time in seconds (optional, defaults to 24 hours)
VITE_TOKEN_EXPIRY=86400

# Feature Flags
# Enable/disable specific features
VITE_ENABLE_ANALYTICS=false

# Development Configuration
# Enable React Query DevTools in development
VITE_ENABLE_DEVTOOLS=true

# Authentication (if needed)
VITE_AUTH_ENABLED=true
VITE_AUTH_TOKEN_KEY=token

# Feature Flags
VITE_ENABLE_EXPORT=true
VITE_ENABLE_IMPORT=true

# Other Configuration
VITE_APP_NAME=OPMAS
VITE_APP_VERSION=1.0.0

# Note: Copy this file to .env and adjust the values as needed
# Do not commit the actual .env file to version control
